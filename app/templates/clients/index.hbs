<h3>Index</h3>

<p>{{link-to 'New client' 'clients.new'}}</p>

<table>
  <tr>
    <th>id</th>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  {{#each}}
    <tr class='client'>
      <td>{{link-to id 'clients.show' this}}</td>
      <td>{{firstName}}</td>
      <td>{{lastName}}</td>
    </tr>
  {{/each}}
</table>
